{
  "name": "ios-test-app",
  "description": "iOS app for testing",
  "keywords": [
    "appium",
    "ios",
    "testapp"
  ],
  "version": "2.12.0",
  "author": "appium",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/appium/ios-test-app.git"
  },
  "bugs": {
    "url": "https://github.com/appium/ios-test-app/issues"
  },
  "engines": [
    "node"
  ],
  "os": [
    "darwin"
  ],
  "main": "build-js/index.js",
  "bin": {},
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "appium-support": "^2.5.0",
    "appium-xcode": "^3.6.0",
    "asyncbox": "^2.3.1",
    "fancy-log": "^1.3.2",
    "rimraf": "^2.5.2",
    "source-map-support": "^0.5.6",
    "teen_process": "^1.5.1"
  },
  "scripts": {
    "clean": "rm -rf node_modules && rm -f package-lock.json && npm install",
    "prepublish": "./node_modules/.bin/gulp prepublish",
    "install": "node install-npm.js",
    "test": "./node_modules/.bin/gulp once",
    "watch": "./node_modules/.bin/gulp",
    "lint": "gulp lint",
    "lint:fix": "gulp lint --fix",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "precommit-test": "gulp lint"
  },
  "pre-commit": [
    "precommit-msg",
    "precommit-test"
  ],
  "devDependencies": {
    "ajv": "^6.5.3",
    "appium-gulp-plugins": "^3.0.0",
    "eslint": "^5.2.0",
    "eslint-config-appium": "^4.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-mocha": "^5.0.0",
    "eslint-plugin-promise": "^4.0.0",
    "gulp": "^4.0.0",
    "pre-commit": "^1.2.2"
  }
}
